{
  "name": "bulkifyai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:cf": "wrangler dev --env staging",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "build:cf": "opennextjs-cloudflare build",
    "build:next": "next build",
    "build:cf:with-env:prod": "cp -f example.env.production .env.production && npm run build:cf",
    "build:cf:with-env:staging": "cp -f example.env.test .env.test && npm run build:cf",
    "preview": "NODE_ENV=production npm run build:cf:with-env:prod && NODE_ENV=production opennextjs-cloudflare preview",
    "preview:staging": "NODE_ENV=test npm run build:cf:with-env:staging && NODE_ENV=test npm run dev:cf",
    "deploy:worker": "npm run deploy --prefix workers/realtime-worker",
    "deploy:worker:staging": "npm run deploy:staging --prefix workers/realtime-worker",
    "deploy": "NODE_ENV=production wrangler deploy --env production --minify",
    "deploy:staging": "NODE_ENV=test npm run build:cf:with-env:staging && NODE_ENV=test wrangler deploy --env staging --minify",
    "deploy:preview": "NODE_ENV=test npm run build:cf:with-env:staging && NODE_ENV=test wrangler deploy --env staging --minify --dry-run",
    "deploy:quick": "NODE_ENV=production wrangler deploy --env production --minify",
    "deploy:staging:quick": "NODE_ENV=test wrangler deploy --env staging --minify",
    "analyze": "npm run build:next && next bundle-analyzer",
    "analyze:watch": "npm run build:next && next bundle-analyzer --watch",
    "analyze:server": "npm run build:next && next bundle-analyzer --server",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "postinstall": "bash ./scripts/setup-git-hooks.sh || true",
    "validate:env": "node scripts/validate-env.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@next/third-parties": "^16.0.0",
    "@opennextjs/cloudflare": "^1.11.0",
    "@radix-ui/react-slot": "^1.2.3",
    "assemblyai": "^4.18.4",
    "audiomotion-analyzer": "^4.5.1",
    "axios": "^1.12.2",
    "axios-logger": "^2.8.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.4",
    "next-seo": "^7.0.1",
    "papaparse": "^5.5.3",
    "react": "19.1.0",
    "react-date-range": "^2.0.1",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/axios": "^0.9.36",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-date-range": "^1.4.10",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "wrangler": "^4.45.0"
  }
}
